{
  "name": "opensdcp-portainer",
  "version": "0.0.1-alpha",
  "description": "Portainer docker container for the OpenSDCP project",
  "main": "docker-compose.yml",
  "scripts": {
    "dev-docker": "docker-compose -f opensdcp-portainer-dev.yml up",
    "start-docker": "docker-compose -f opensdcp-portainer-prod.yml up",
    "deploy-docker":
      "docker stack deploy -c opensdcp-portainer-prod.yml opensdcp-portainer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opensdcp/opensdcp-portainer.git"
  },
  "author": "Felicitas Pojtinger",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/opensdcp/opensdcp-portainer/issues"
  },
  "homepage": "https://github.com/opensdcp/opensdcp-portainer#readme"
}
